{
  "expo": {
    "name": "조선몽록",
    "slug": "project-tg",
    "version": "1.0.1",
    "orientation": "portrait",
    "icon": "./assets/appicon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "fonts": [
      "./assets/fonts/MaruBuri-ExtraLight.ttf",
      "./assets/fonts/MaruBuri-Light.ttf",
      "./assets/fonts/MaruBuri-Regular.ttf",
      "./assets/fonts/MaruBuri-SemiBold.ttf",
      "./assets/fonts/MaruBuri-Bold.ttf"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.moledrum.textgame",
      "buildNumber": "2",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.moledrum.textgame"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "updates": {
      "enabled": true,
      "checkAutomatically": "ON_ERROR_RECOVERY",
      "fallbackToCacheTimeout": 0
    },
    "extra": {
      "eas": {
        "projectId": "c034f295-efc3-4a34-8335-dc754c4f0fa9"
      }
    },
    "owner": "4losers",
    "plugins": [
      "expo-font"
    ]
  }
}